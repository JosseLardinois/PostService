    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>91c9415d-be19-4302-ba85-f44ec10e5928</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
